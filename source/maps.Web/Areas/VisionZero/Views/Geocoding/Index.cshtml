@{
    ViewBag.Title = "Geocoding - Vision Zero - maps.if.ua";
    Layout = "~/Areas/VisionZero/Views/Shared/_Layout.cshtml";
}
@section styles {
    @Styles.Render("~/Content/css/vision-zero.css")
}
@section header
{
    <script src="~/Scripts/underscore.min.js"></script>
    @Scripts.Render("~/Scripts/vision-zero/geocoding.js")
}
<div class="container vision-zero">
    <div class="row">
        <div class="col-lg-12">
            <form class="form">
                <div class="form-group">
                    <label>Input:</label>
                    <textarea rows="12" class="form-control" id="Input"></textarea>
                </div>
                <div class="btn btn-primary" id="ProcessBtn">Process</div>
                <div class="form-group">
                    <label>Output:</label>
                    <textarea rows="12" class="form-control"  id="Output"></textarea>
                </div>
               
            </form>
        </div>
    </div>
</div>